{
  "name": "nuvoleweb/drupal-behat",
  "description": "Contexts and other components for the Drupal Behat extension.",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "drupal/drupal-extension": "~3.2",
    "phpunit/phpunit": "5.5.*"
  },
  "require-dev": {
    "composer/installers": "1.1.0",
    "drupal-composer/drupal-scaffold": "dev-master",
    "drupal/coder": "8.2.8",
    "drush/drush": "~8.1",
    "drupal/core": "~8.1",
    "drupal/chosen": "~8.2",
    "drupal/coffee": "dev-8.x-1.x",
    "drupal/config_installer": "~8.1",
    "bovigo/assert": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "NuvoleWeb\\Drupal\\": "./src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "bash ./scripts/setup.sh"
    ],
    "phpcs": "./vendor/bin/phpcs --standard=Drupal,DrupalPractice ./src",
    "phpcbf": "./vendor/bin/phpcbf --standard=Drupal,DrupalPractice ./src"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    },
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.5.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.5.0/chosen_v1.5.0.zip",
          "type": "zip"
        }
      }
    }
  ],
  "extra": {
    "installer-paths": {
      "drupal/core": [
        "type:drupal-core"
      ],
      "drupal/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "drupal/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "drupal/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drupal/libraries/{$name}": [
        "type:drupal-library"
      ]
    }
  }
}
